Module Factorial
    Sub Main()
        Dim number As String, fact As Integer
        Console.WriteLine("Insert number for factoriel:")
        number = Console.ReadLine
        fact = Factorial(number)
        Console.Write("Factorial of a number: {0}", fact)
        Console.ReadKey()
    End Sub

    Private Function Factorial(ByVal num As Integer) As Integer
        If num = 1 Then
            Return 1
        Else
            Return num * Factorial(num - 1)
        End If
         Return num
    End Function
End Module
