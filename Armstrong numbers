Imports System.IO
Module Module1

    Sub Main()
        Dim num, n, sum, digit, res As Integer
        Console.WriteLine("Insert number:")
        num = Console.ReadLine
        n = num
        sum = 0

        While num <> 0
            digit = num Mod 10
            res = Math.Pow(digit, 3)
            sum += res
            num = Conversion.Int(num \ 10)
        End While
        
        If sum = n Then
            Console.WriteLine("The number is Armstrong Number!")
        Else
            Console.WriteLine("Not Armstrong number")
        End If

        Console.ReadLine()
    End Sub
End Module


