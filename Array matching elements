Module ArrayComparing
'Comparing elements of two arrays to check which colours belongs to both arrays 
    Sub Main()
        Dim colours1(),colours2(),match As String
        Dim i, j As Integer
        
        colours1 = {"red", "blue", "black", "orange", "yellow", "pink", "grey"}
        colours2 = {"brown", "white", "orange", "green", "red", "purple", "black"}

        i = 0
        While i <= colours2.Length - 1
            For j = 0 To colours1.Length - 1
                If colours2(i) = colours1(j) Then
                    match = colours2(i)
                    Console.WriteLine("Matches: {0}", match)
                End If
            Next
            i += 1
        End While
        Console.ReadKey()
    End Sub
End Module
