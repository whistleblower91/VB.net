Module ArrayComparing
REM Comparing elements of two arrays to check which colours belongs to both arrays 
    Sub Main()
        Dim colours1() As String = {"red", "blue", "black", "orange", "yellow", "pink", "grey"}
        Dim colours2() As String = {"brown", "white", "orange", "green", "red", "purple", "black"}
        Dim i, j As Integer, match As String

        i = 0
        While i <= colours2.Length - 1
            For j = 0 To colours1.Length - 1
                If colours2(i) = colours1(j) Then
                    match = colours2(i)
                    Console.WriteLine("Matches: {0}", match)
                End If
            Next
            i += 1
        End While
        Console.ReadKey()
    End Sub
End Module
