Module Module1
    Sub Main()
        Dim num As Long
        Dim per, res As Integer
        Console.WriteLine("Insert number:")
        num = Console.ReadLine()
        Console.WriteLine("Insert percentage:")
        per = Console.ReadLine()
        res = num * per \ Math.Pow(10, 2)
        Console.WriteLine("Percentage: {0}",res)
        Console.ReadKey()
    End Sub
End Module
